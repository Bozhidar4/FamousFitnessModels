@{
    ViewBag.Title = "Home";
}

<div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="4000">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
        <li data-target="#myCarousel" data-slide-to="3"></li>
        <li data-target="#myCarousel" data-slide-to="4"></li>
    </ol>
    <div class="carousel-inner" role="listbox">
        <div class="item active">
            <img src="http://wallsdesk.com/wp-content/uploads/2016/02/Lazar-Angelov-Wallpapers.jpg" class="img-responsive img-custom" />
            <div class="carousel-caption" role="option">
                <p>
                    Lazar Angelov
                </p>
            </div>
        </div>
        <div class="item">
            <img src="~/Images/max2.jpg" class="img-responsive" />
            <div class="carousel-caption" role="option">
                <p>
                    Max "The Body" Philisaire
                </p>
            </div>
        </div>
        <div class="item">
            <img src="~/Images/ulisses3.jpg" class="img-responsive" />
            <div class="carousel-caption" role="option">
                <p>
                    Ulisses Jr
                    @*<a class="btn btn-default" href="http://go.microsoft.com/fwlink/?LinkID=525029&clcid=0x409">
                            Learn More
                        </a>*@
                </p>
            </div>
        </div>
        <div class="item">
            <img src="https://www.sergiconstance.com/wp-content/uploads/2014/11/021.jpg" class="img-responsive" />
            <div class="carousel-caption" role="option">
                <p>
                    Sergi Constance
                </p>
            </div>
        </div>
        <div class="item">
            <img src="https://i.ytimg.com/vi/scAIOEy4jpY/maxresdefault.jpg" class="img-responsive" />
            <div class="carousel-caption" role="option">
                <p>
                    Simeon Panda
                </p>
            </div>
        </div>
    </div>
    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>

